      "type": "registration",
      "name": "RangerRegistration",
      "terms": {
        "version": "1.0.0",
        "hash": "sha256:deadbeef"
      },
      "roles": {
        "owner": "owner-address-placeholder",
        "supreme_admin": "supreme-admin-placeholder",
