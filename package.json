{
  "name": "rangerplex-ai",
  "version": "2.13.11",
  "type": "module",
  "scripts": {
    "start": "npm run stop 2>/dev/null; concurrently \"npm run server\" \"npm run dev\" --names \"SERVER,VITE\" --prefix-colors \"cyan,magenta\"",
    "stop": "bash scripts/shutdown.sh",
    "pm2:start": "pm2 start ecosystem.config.cjs",
    "pm2:stop": "pm2 stop ecosystem.config.cjs",
    "pm2:restart": "pm2 restart ecosystem.config.cjs",
    "pm2:reload": "pm2 reload ecosystem.config.cjs",
    "pm2:logs": "pm2 logs",
    "pm2:status": "pm2 status",
    "pm2:update": "pm2 update",
    "dev": "vite --host",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "proxy": "node proxy_server.js",
    "server": "node proxy_server.js",
    "browser": "npm run stop 2>/dev/null; node scripts/launch_browser.cjs"
  },
  "dependencies": {
    "@google/genai": "^1.30.0",
    "@monaco-editor/react": "^4.7.0",
