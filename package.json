    "cors": "^2.8.5",
    "date-fns": "^4.1.0",
    "dompurify": "^3.3.0",
    "exif-parser": "^0.1.12",
    "exif-reader": "^2.0.2",
    "express": "^4.19.2",
    "idb": "^8.0.0",
    "lucide-react": "^0.294.0",
    "mammoth": "^1.6.0",
    "mermaid": "^10.9.0",
    "node-fetch": "^3.3.2",
    "node-pty": "^1.0.0",
    "openmeteo": "^1.2.2",
    "pdfjs-dist": "^4.0.379",
    "puppeteer": "^24.31.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-markdown": "^9.0.1",
    "recharts": "^2.12.7",
    "remark-gfm": "^4.0.0",
    "rss-parser": "^3.13.0",
    "turndown": "^7.1.2",
    "uuid": "^9.0.1",
    "ws": "^8.14.0",
    "xterm": "^5.3.0",
    "xterm-addon-fit": "^0.8.0",
    "xterm-addon-web-links": "^0.9.0"
